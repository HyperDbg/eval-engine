#include <stdio.h>
#include "scanner.h"

int main()
{
    char c = '\0';
    FILE *InFile = fopen("input.txt", "r"); //read only file
    // check file existance
    if (InFile == NULL)
    {
        printf("Error: Could Not open file\n");
        return -1;
    }

    while (c != EOF)
    {
        c = getc(InFile);
        printf("%c:%x\n ", c, c);
    }
    fclose(InFile);

    return 0;
}